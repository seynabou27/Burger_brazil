{% block body %}

    {% if success is defined and success !='' %}
        <div id="message" class="    alert-success" role="alert">
            {{success}}
        </div>
        {% endif %}
        <span class="d-none">{{removeSucess}}</span>

    
    <body>

      
        <div class="container-fluid" id="fluid">
            <h4 class="annoce">
                Bienvenue au Temple du Burger !!
            </h4>
            <img src="images/catalogue1.png" alt="jijiuj">


        </div>
        <div class="contente">
            <div class="row ensemble">
                <div class="col-lg-4 col-sm-6">
                    <div class="thumbnail gauche">
                        <img src="images/gauche.png" />
                    </div>
                </div>
    
                <div class="col-lg-4 col-sm-6">
                    <div class="thumbnail milieu">
                        <img src="images/milieu.png" />
                    </div>
                </div>
                <div class="col-lg-4 col-sm-12">
                    <div class="thumbnail droite">
                        <img src="images/droite.png" />
                    </div>
                </div>
                
                
            </div>

            <div class="ensemble2">
                <h5>
                    QUE SOUHAITEZ-VOUS ?
                </h5>
                <p>
                    Découvrez notre carte de burgers simples, de menus et
                    <br> de délicieux compléments !!
                </p>
    
            </div>
            <!--menu card-->

            <div class="container filtres">
                <!--filtering-->

                <ul class=" .food_section filters_menu">
                    <li class="active" data-filter="*">Tout</li>
                    <li data-filter=".Menus" class="">Menus</li>
                    <li data-filter=".burger" class="">Burger</li>
                </ul>
 
                <div class="grid" style="display:flex;">
                    <div class="container contenu2 mx-auto mt-4">
                        

                        <div class="row cards" style=" margin-right: 48px; margin-left: 63px;" >
                            {% for catalogues in catalogues %}

                                {# {% if type is defined and type == "menu" %}
                                    {% for row in catalogues.imagess %}
                                        <img src="/images/{{row.nom}}" alt="jfugzurg" style="width: 100%;">
                                    {% endfor %}
                                    {% else %}
                                    {% for row in catalogues.images %}
                                        <img src="/images/{{row.nom}}" alt="jfugzurg" style="width: 100%;">
                                    {% endfor %}
                                {% endif %} #}
                            {# {% for image in menus.imagess %} #}
            
                            <div class="col-md-4 menus mt-4">
                                <div class="card" style="width: 300px; display:flex;">
                                    <img src="/images/burger2.png" class="card-img-top pulse" alt="...">
                                    <div class="card-body">
                                        <h5 class="card-title">{{catalogues.nom}}</h5>
                                        <h6 class="card-subtitle mb-2 text-muted"><p class="MN">Menu . {{catalogues.prix}} Fcfa</h6></p>
                                        <a href="{{path('details',{'id':catalogues.id})}}" class="btn details " style="margin-left: 89px;"><i class="fab fa-add"></i> Voir Détails</a>
                                        <a class="panier" role="button" id="panier" href="{{path('add_panier', {'id':catalogues.id ~ catalogues.type})}}"><i class="fa-solid fa-cart-shopping" style="color: #FF9D19 ;margin-right: 15px;"></i></a>
                                        
                                    </div>
                                </div>
                            </div>    
                            
                
                    
                                {# {% endfor %}  #}
                            {% endfor %}
                        </div>
                        
                       
                    </div>
                    

                </div>
                

                <a href="{{ path('app_login') }}" class="btn btn-primary btn-subtle" id="commander" role="button" aria-disabled="true">PASSER UNE COMMANDE</a>

            </div>

